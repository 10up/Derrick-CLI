{
  "name": "moonshine-cli",
  "version": "0.1.0",
  "description": "Command line interface to moonshine",
  "main": "./src/index.js",
  "scripts": {
    "build:bin": "jshint bin/",
    "watch:bin": "watch \"npm run build:bin\" bin/",
    "build:src": "jshint src/",
    "watch:src": "watch \"npm run build:src\" src/",
    "build": "npm run build:bin && npm run build:src",
    "build:watch": "parallelshell \"npm run watch:bin\" \"npm run watch:src\""
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:johnpbloch/moonshine-cli.git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "jshint": "^2.5.11",
    "parallelshell": "^1.0.3",
    "watch": "^0.13.0"
  },
  "bin": {
    "moonshine": "./bin/moonshine.js"
  },
  "dependencies": {
    "socket.io-client": "^1.2.1"
  }
}
