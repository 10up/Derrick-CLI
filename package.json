      "name": "Taylor Lovett",
      "email": "taylor.lovett@10up.com",
      "url": "https://github.com/tlovett1"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "^0.11.2",
    "grunt-mocha-test": "^0.12.7",
    "istanbul": "^0.3.14",
    "jshint": "^2.5.11",
    "matchdep": "^0.3.0",
    "mocha": "^2.2.5",
    "proxyquire": "^1.5.0",
    "tar.gz": "^0.1.1",
    "watch": "^0.13.0"
  },
  "bin": {
    "derrick": "./bin/derrick"
  },
  "dependencies": {
    "cli-spinner": "^0.2.0",
    "cli-table": "^0.3.1",
    "compare-version": "^0.1.2",
    "debug": "^2.2.0",
    "deferred": "^0.7.2",
    "flatiron": "^0.4.3",
    "lodash": "^3.7.0",
    "minimist": "^1.1.1",
    "mkdirp": "^0.5.0",
    "mustache": "^2.0.0",
    "path-is-absolute": "^1.0.0",
    "progress": "^1.1.8",
    "promise": "^7.0.1",
    "prompt": "^0.2.14",
    "request": "^2.55.0",
    "socket.io-client": "^1.3.5",
    "tar": "^2.1.0",
    "tar.gz": "^0.1.1",
    "temp": "^0.8.1",
    "unzip": "^0.1.11",
    "winston": "^0.8.3"
  }
}
